<header class="site-header" role="banner">
  <div class="nav-header navbar-default">
    <div class="container-fluid container">
      {%- assign default_paths = site.pages | map: "path" -%} {%- assign page_paths = site.header_pages | default: default_paths
      -%}
      <a class="site-title navbar-brand" rel="author" href="{{ "/" | relative_url }}">
        <img src="{{" /assets/logo.png "}}" alt="logo" />
      </a>

      {%- if page_paths -%}
      <nav class="site-nav">

        <div class="trigger">
          <ul class="nav navbar-nav">
          {%- for path in page_paths -%} {%- assign my_page = site.pages | where: "path", path | first -%} {%- if my_page.title -%}
          <li><a class="page-link" href="{{ my_page.url | relative_url }}">{{ my_page.title | escape }}</a></li>
          {%- endif -%} {%- endfor -%}
          <li><a class="page-link" style="color: rgb(40, 117, 196)"href="/whitepaper.pdf"><i class="far fa-file"></i> Whitepaper</a></li>
            </ul>
        </div>
      </nav>
      {%- endif -%}
    </div>
  </div>
</header>
